    {
      "name": "Server start",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go", // Path to your main.go file
      "args": ["serve"], // Replace with your Cobra command and flags
      "console": "integratedTerminal"
    },
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}"
    },
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${file}"
    }
